{
  "name": "honeycoin-audit-trail",
  "version": "1.0.0",
  "description": "Advanced audit trail system for HoneyCoin financial transactions",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "setup-db": "psql -d honeycoin -f database-setup.sql",
    "load-test-data": "sudo -u postgres psql -d honeycoin -f performance-test-data.sql",
    "setup-refunds-reversals": "sudo -u postgres psql -d honeycoin -f refunds-reversals-enhancement.sql",
    "performance-test": "node performance-test.js"
  },
  "keywords": ["audit", "blockchain", "fintech", "postgresql", "express"],
  "author": "HoneyCoin Candidate",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}